includes:
  - ./vendor/larastan/larastan/extension.neon

parameters:
  level: max
  paths:
    - app
    - tests

  excludePaths:
    - bootstrap/*
    - storage/*
    - vendor/*
    - database/migrations/*
    - public/*

  ignoreErrors:
    - '#Method .* should return Illuminate\\Database\\Eloquent\\Relations\\.*#'

  checkUninitializedProperties: true
